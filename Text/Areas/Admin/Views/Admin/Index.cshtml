@using Text.Models

@{ 
    Member member = (Member)Session["UserLogin"];
}

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="wrapper">
    <div class="row">              
        <div class="col-8 col-m-12 col-sm-12">
            <div class="card">
                <div class="card-header">
                    <h3>
                        Welcome @member.lastName @member.firstName
                    </h3>
                    <br />
                    <h3>
                        Role: @ViewBag.RoleName
                    </h3>
                    
                </div>
                
                <div class="col-4 col-m-12 col-sm-12">
                    <img src="~/Areas/Admin/Content/image/BannerAdmin.jpg" />
                </div>
            </div>
        </div>
    </div>
</div>

